<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.cci.gpec.db">
	<class
		name="Formation"
		table="FORMATION"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="ID_FORMATION"
		>
			<generator class="identity"/>
		</id>

		<property
			name="DebutFormation"
			column="DEBUT_FORMATION"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="FinFormation"
			column="FIN_FORMATION"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="NomFormation"
			column="NOM_FORMATION"
			type="text"
			not-null="false"
		/>
		<property
			name="OrganismeFormatino"
			column="ORGANISME_FORMATINO"
			type="text"
			not-null="false"
		/>
		<property
			name="Mode"
			column="MODE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="VolumeHoraire"
			column="VOLUME_HORAIRE"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Dif"
			column="DIF"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="HorsDif"
			column="HORS_DIF"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NombreJourOuvre"
			column="NOMBRE_JOUR_OUVRE"
			type="double"
			not-null="false"
			length="10"
		/>
		<property
			name="Justificatif"
			column="JUSTIFICATIF"
			type="text"
			not-null="false"
		/>
		<property
			name="CoutOpca"
			column="COUT_OPCA"
			type="double"
			not-null="false"
			length="10"
		/>
		<property
			name="CoutEntreprise"
			column="COUT_ENTREPRISE"
			type="double"
			not-null="false"
			length="10"
		/>
		<property
			name="CoutAutre"
			column="COUT_AUTRE"
			type="double"
			not-null="false"
			length="10"
		/>
		<many-to-one
			name="DomaineFormation"
			column="ID_DOMAINE_FORMATION"
			class="Domaineformation"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Salarie"
			column="ID_SALARIE"
			class="Salarie"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			class="Absence"
			name="Absence"
			not-null="false"
			column="ID_ABSENCE" 
		>
		</many-to-one>


	</class>	
</hibernate-mapping>