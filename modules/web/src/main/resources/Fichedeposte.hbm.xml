<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.cci.gpec.db">
	<class name="FicheDePoste" table="FICHEDEPOSTE">
		
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="ID_FICHE_DE_POSTE"
		>
			<generator class="identity"/>
		</id>
		

		<many-to-one
			class="Salarie"
			column="ID_SALARIE"
			name="Salarie"
			not-null="false"
		 >
		 </many-to-one>
		<many-to-one
			class="FicheMetier"
			column="ID_METIER_TYPE"
			name="MetierType"
			not-null="false"
		 >
		 </many-to-one>

		<property
			column="DATE_MODIFICATION"
			length="10"
			name="DateModification"
			not-null="false"
			type="date"
		 />
		<property
			column="COMPETENCES_SPECIFIQUES_TEXTE"
			name="CompetencesSpecifiquesTexte"
			not-null="false"
			type="text"
		 />
		 <property
			column="COMPETENCES_SPECIFIQUES"
			name="CompetencesSpecifiques"
			not-null="false"
			type="text"
		 />
		 <property
			column="SAVOIR"
			name="Savoir"
			not-null="false"
			type="text"
		 />
		  <property
			column="SAVOIR_FAIRE"
			name="SavoirFaire"
			not-null="false"
			type="text"
		 />
		  <property
			column="SAVOIR_ETRE"
			name="SavoirEtre"
			not-null="false"
			type="text"
		 />
		  <property
			column="EVALUATION_GLOBALE"
			name="EvaluationGlobale"
			not-null="false"
			type="text"
		 />
		 <property
			column="CATEGORIE_COMPETENCE"
			name="CategorieCompetence"
			not-null="false"
			type="text"
		 />
		  <property
			column="CATEGORIE_COMPETENCE2"
			name="CategorieCompetence2"
			not-null="false"
			type="text"
		 />
		  <property
			column="CATEGORIE_COMPETENCE3"
			name="CategorieCompetence3"
			not-null="false"
			type="text"
		 />
		  <property
			column="CATEGORIE_COMPETENCE4"
			name="CategorieCompetence4"
			not-null="false"
			type="text"
		 />
		  <property
			column="CATEGORIE_COMPETENCE5"
			name="CategorieCompetence5"
			not-null="false"
			type="text"
		 />
		  <property
			column="MOBILITE_ENVISAGEE"
			name="MobiliteEnvisagee"
			not-null="false"
			type="text"
		 />
			
		<property
			column="COMMENTAIRES"
			name="Commentaires"
			not-null="false"
			type="text"
		 />
		<property
			column="DATE_CREATION"
			length="10"
			name="DateCreation"
			not-null="false"
			type="date"
		 />
		<property
			column="COMPETENCES_NOUVELLES"
			name="CompetencesNouvelles"
			not-null="false"
			type="text"
		 />
		<property
			column="COMPETENCES_NOUVELLES2"
			name="CompetencesNouvelles2"
			not-null="false"
			type="text"
		 />
		<property
			column="COMPETENCES_NOUVELLES3"
			name="CompetencesNouvelles3"
			not-null="false"
			type="text"
		 />
		<property
			column="COMPETENCES_NOUVELLES4"
			name="CompetencesNouvelles4"
			not-null="false"
			type="text"
		 />
		<property
			column="COMPETENCES_NOUVELLES5"
			name="CompetencesNouvelles5"
			not-null="false"
			type="text"
		 />		
		<property
			column="ACTIVITES_SPECIFIQUES"
			name="ActivitesSpecifiques"
			not-null="false"
			type="text"
		 />
		 <property
			name="Justificatif"
			column="JUSTIFICATIF"
			type="text"
			not-null="false"
		/>
	</class>
</hibernate-mapping>
